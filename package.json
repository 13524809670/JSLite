{
  "name": "jslite",
  "version": "1.1.10",
  "description": "JSLite is a minimalist JavaScript library for modern browsers with a largely jQuery-compatible API. If you use jQuery, you already know how to use JSLite.",
  "main": "dist/JSLite.js",
  "homepage": "https://github.com/JSLite/JSLite",
  "repository": {
    "type": "git",
    "url": "https://github.com/JSLite/JSLite.git"
  },
  "keywords": [
    "JSLite",
    "jslite",
    "library",
    "browser",
    "javascript"
  ],
  "author": "kenny wang <wowohoo@qq.com> (http://JSLite.io)",
  "license": {
    "type": "MIT",
    "url": "http://jslite.io/license/"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": "js"
    }
  },
  "scripts": {
    "test": "mocha-phantomjs test/index.html",
    "lint": "eslint src/core/init.js",
    "build": "node ./build/build.js",
    "dev": "webpack --watch --config build/webpack.dev.config.js"
  },
  "bugs": {
    "url": "https://github.com/JSLite/JSLite/issues"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-runtime": "^6.6.1",
    "bannerjs": "^1.0.1",
    "chai": "^3.5.0",
    "eslint": "^2.13.1",
    "mocha": "^2.4.5",
    "mocha-phantomjs": "^4.0.2",
    "rollup": "^0.25.4",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-replace": "^1.1.0",
    "uglify-js": "^2.6.2",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
